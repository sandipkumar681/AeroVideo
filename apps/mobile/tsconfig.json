{
  "extends": ["expo/tsconfig.base", "../../tsconfig.base.json"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@aerovideo/types": ["../../packages/types/src/index.ts"],
      "@aerovideo/schemas": ["../../packages/schemas/src/index.ts"],
      "@aerovideo/utils": ["../../packages/utils/src/index.ts"],
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "types/**/*.d.ts"
  ]
}
